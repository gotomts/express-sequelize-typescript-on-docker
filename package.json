{
  "name": "express-typeorm-typescript",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' ./src/bin/www.ts",
    "test": "jest",
    "migration:generate": "ts-node ./node_modules/.bin/typeorm migration:generate",
    "migration:run": "ts-node ./node_modules/.bin/typeorm migration:run",
    "migration:revert": "ts-node ./node_modules/.bin/typeorm migration:revert",
    "migration:create": "ts-node ./node_modules/.bin/typeorm migration:create -n"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mysql2": "^2.2.5",
    "pug": "2.0.0-beta11",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.28"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.8",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.14",
    "@types/morgan": "^1.9.1",
    "@types/node": "^8.0.29",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "babel-jest": "^26.3.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.6",
    "ts-node": "^3.3.0",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.0.3"
  },
  "eslintIgnore": [
    "/.eslintrc.js"
  ]
}
